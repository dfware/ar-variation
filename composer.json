{
    "name": "dfware/ar-variation",
    "description": "Provides support for ActiveRecord variation via related models in Yii2",
    "keywords": ["yii2", "active", "record", "variation", "variator", "link", "many", "many-to-many", "many to many", "translation", "i18n"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dfware/ar-variation/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/dfware/ar-variation/wiki",
        "source": "https://github.com/dfware/ar-variation"
    },
    "authors": [
        {
            "name": "Pavel Karelin",
            "email": "devpastet@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.27|^5.0|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"dfware\\ar\\variation\\": "src"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
